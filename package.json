{
  "name": "kamyroll",
  "version": "0.3.2",
  "description": "Tauri app that uses the Kamyroll API to watch animes",
  "keywords": [
    "vite",
    "tauri",
    "vue",
    "vuetify",
    "desktop"
  ],
  "scripts": {
    "lint": "exit 0;",
    "serve": "vite --port 3000",
    "serve:native": "VITE_HTTP_HANDLER=NATIVE vite --port 8080",
    "serve:tauri": "tauri dev",
    "build:web": "vite build",
    "build": "vite build && tauri build --debug"
  },
  "dependencies": {
    "@egoist/vue-emotion": "^0.6.2",
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.10.4",
    "@mdi/font": "7.0.96",
    "artplayer": "^4.5.6",
    "execa": "^6.1.0",
    "hls.js": "^1.2.3",
    "roboto-fontface": "0.10.0",
    "tauri-plugin-store-api": "github:tauri-apps/tauri-plugin-store#dev",
    "twin.macro": "^2.8.2",
    "vite-plugin-vuetify": "^1.0.0-alpha.17",
    "vue": "3.2.39",
    "vue-global-api": "0.4.1",
    "vue-router": "4.1.5",
    "vuetify": "^3.0.0-alpha.11",
    "webpack-plugin-vuetify": "^2.0.0-alpha.13"
  },
  "devDependencies": {
    "@tauri-apps/api": "^1.1.0",
    "@tauri-apps/cli": "^1.1.1",
    "@vitejs/plugin-vue": "3.1.0",
    "vite-plugin-vuetify": "^1.0.0-alpha.17",
    "autoprefixer": "^10.4.11",
    "postcss": "^8.4.16",
    "sass": "1.54.9",
    "sass-loader": "13.0.2",
    "tailwindcss": "^3.1.8",
    "video.js": "^7.20.3",
    "vite": "^3.1.3"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/Bilnaa/kamyroll-tauri/"
  },
  "license": "ISC"
}
